{
  "name": "project-viewer",
  "main": "./src/main",
  "version": "0.9.0",
  "description": "Project viewer/manager that lets you add, edit and remove clients/groups/projects as well as switching between them.",
  "keywords": [
    "project",
    "productivity",
    "settings",
    "workflow"
  ],
  "repository": "https://github.com/jccguimaraes/atom-project-viewer",
  "license": "MIT",
  "engines": {
    "atom": ">=1.12.7"
  },
  "dependencies": {
    "atom-space-pen-views": "latest",
    "devicons": "1.8.0"
  },
  "devDependencies": {
    "all-contributors-cli": "latest",
    "eslint": "latest",
    "eslint-plugin-jasmine": "latest"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "provideStatusBar"
      }
    }
  },
  "providedServices": {
    "project-viewer": {
      "description": "Provides an API service for managing outside groups and items",
      "versions": {
        "1.0.0": "projectViewerService"
      }
    }
  },
  "scripts": {
    "linter": "eslint src/** && eslint spec/**"
  }
}
